{
  "name": "update_annotation_tool",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.1.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "antd": "^4.16.3",
    "async": "^3.2.0",
    "axios": "^0.21.1",
    "craco-less": "^1.17.1",
    "i18next": "^20.3.2",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-http-backend": "^1.2.6",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.11.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "set PORT=3006 && craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "quotes": [
        2,
        "double"
      ],
      "semi": [
        2,
        "always"
      ],
      "curly": [
        2,
        "all"
      ],
      "camelcase": [
        2,
        {
          "properties": "always"
        }
      ],
      "eqeqeq": [
        2,
        "smart"
      ],
      "one-var-declaration-per-line": [
        2,
        "always"
      ],
      "new-cap": 2,
      "no-case-declarations": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "cracoConfig": "src/configs/craco.js",
  "devDependencies": {
    "react-csv": "^2.0.3"
  }
}
